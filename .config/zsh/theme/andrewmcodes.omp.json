{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#2b2f31",
          "foreground": "#4cc38a",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": "\ue5ff {{ .Path }}",
          "trailing_diamond": "\ue0b4",
          "type": "path"
        },
        {
          "background": "#2b2f31",
          "foreground": "#52a9ff",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff8b3e{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f16a50{{ end }}",
            "{{ if gt .Ahead 0 }}#9e8cfc{{ end }}",
            "{{ if gt .Behind 0 }}#9e8cfc{{ end }}"
          ],
          "leading_diamond": " \ue0b6",
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "trailing_diamond": "\ue0b4",
          "type": "git"
        },
        {
          "background": "#2b2f31",
          "foreground": "#f5d90a",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "python"
        },
        {
          "background": "#2b2f31",
          "foreground": "#42E66C",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": true,
            "fetch_package_manager": true,
            "yarn_icon": "<#348cba>\uf61a</>"
          },
          "style": "diamond",
          "template": "\ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "node"
        },
        {
          "background": "#2b2f31",
          "foreground": "#ff6369",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": true
          },
          "style": "diamond",
          "template": " \ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "ruby"
        },
        {
          "background": "#2b2f31",
          "foreground": "#336791",
          "leading_diamond": " \ue0b6",
          "properties": {
            "shell": "zsh",
            "command": "pg_isready | psql --version | psql --version | awk '{print $3}'"
          },
          "style": "diamond",
          "template": " \ue76e {{ .Output }}",
          "trailing_diamond": "\ue0b4",
          "type": "command"
        },
        {
          "background": "#2b2f31",
          "foreground": "#ff6369",
          "leading_diamond": " \ue0b6",
          "properties": {
            "shell": "zsh",
            "command": "redis-cli ping | grep -q PONG && redis-cli info | grep -m1 -o 'redis_version:[0-9]*\\.[0-9]*' | cut -d: -f2"
          },
          "style": "diamond",
          "template": " \ue76d {{ .Output }}",
          "trailing_diamond": "\ue0b4",
          "type": "command"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#2b2f31",
          "foreground": "#9ba1a6",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "austin",
            "threshold": 150
          },
          "style": "diamond",
          "template": "{{ .FormattedMs }}",
          "trailing_diamond": "\ue0b4 ",
          "type": "executiontime"
        },
        {
          "background": "#2b2f31",
          "foreground": "#00c2d7",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ .Path }} \u276f",
          "trailing_diamond": "\ue0b4",
          "type": "path"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
