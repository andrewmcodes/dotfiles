  // 	],
  // 	"description": "Log output to console"
  // }
  "Add new data to Appearances JSON file": {
    "prefix": "appearance",
    "body": [
      "{",
      "\t\"category\": \"${1|podcast,livestream,conference,panel|}\",",
      "\t\"icon\": \"${2|speaker_loud,video,chat,avatar|}\",",
      "\t\"title\": \"${3:title_value}\",",
      "\t\"date\": \"${4:date_value}\",",
      "\t\"url\": \"${5:url_value}\",",
      "\t\"cta\": \"${6|Listen to podcast,Watch video,Watch livestream|}\",",
      "\t\"event\": \"${7:event_value}\",",
      "\t\"description\": \"${8:description_value}\"",
      "}"
    ],
    "description": "Adds a new item to a JSON file with the specified properties"
  }
}
